<div class="container">

 <%= render 'layouts/error_messages', model: @post %>
 
 <div class="row justify-content-center">
 <h1 class="main-title mb-2">Edit</h1>
 </div>
 
<div class="row justify-content-center">


<%= form_with model: @post, url: post_path(@post.id), method: :patch do |f| %>

<%= render partial: 'form', locals: {f:f } %>

 <div class="actions m-3">
 <%= f.submit 'Up date', class: "btn btn-success" %>
</div>


<% end %>

</div>
</div>